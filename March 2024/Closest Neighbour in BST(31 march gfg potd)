class Solution {
  public:
    int findMaxForN(Node* root, int n) {
        // code here
          if(!root)
            return -1;
            
        if(root->key > n)
            return findMaxForN(root->left, n);
            
        if(root->key <= n)
            return max(root->key, findMaxForN(root->right, n));
    }
};
